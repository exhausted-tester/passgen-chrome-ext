{
  "manifest_version": 3,
  "name": "Random Password Generator",
  "short_name": "RPGen",
  "version": "1.0",
  "description": "Generate random passwords with customizable options.",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon128.png"
  },
  "permissions": ["clipboardWrite"]
}
